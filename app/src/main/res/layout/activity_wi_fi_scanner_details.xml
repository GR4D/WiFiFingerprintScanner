<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".WiFiScannerDetails">

    <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarDetails"
            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            tools:layout_editor_absoluteY="3dp"/>
    <TextView
            android:layout_width="379dp"
            android:layout_height="125dp" android:id="@+id/scanDetails"
            tools:layout_editor_absoluteX="16dp" android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/toolbarDetails"/>
</androidx.constraintlayout.widget.ConstraintLayout>